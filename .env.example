[env]
# OTA Update Password - change this to a secure password
OTA_PASSWORD = your_secure_password_here

# WiFi Credentials - default connection (optional if using wifi_config.json)
WIFI_SSID = YourWiFiSSID
WIFI_PASSWORD = YourWiFiPassword

# Access Point Credentials - fallback mode when WiFi fails
AP_SSID = ESP2GO
AP_PASSWORD = 12345678

MDNS_HOSTNAME = esp2go.local